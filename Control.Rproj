    if(t>=1 & t<=800){ #Primeros 800 datos bajo control
      datosf2<-rnorm(5,0,1) 
    }
    if(t > 800 & t <= 820){
      datosf2<-rnorm(5,0.3,1) 
    }
    if(t>820){
