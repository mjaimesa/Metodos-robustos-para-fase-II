    if(t>=1 & t<=800){ #Primeros 800 datos bajo control
      datosf2<-rnorm(5,0,1) 
    }
    if(t > 800 & t <= 980){
      datosf2<-rnorm(5,1,1) 
    }
    if(t>980){
      datosf2<-rnorm(5,0,1)
    }
    
    if(mean(datosf2)<UCLtm1 & mean(datosf2)>LCLtm1){
      mediasf214<-c(mediasf214,mean(datosf2))
      varf214<-c(varf214,var(datosf2))
      m2<-m2+1
    } else {
      if(t>=1 & t<=800){
        fa<-fa+1 #falsa alarma porque est치 bajo control
      }
      if(t > 800 & t<=980){
        cs<-cs+1 #se침al correcta porque est치 fuera de control
      }
      if(t>980){
        fa<-fa+1 #falsa alarma porque est치 bajo control
      }
    }
  }
  cfap14mr[i]<-fa/1620 #falsas alarmas
  ctap14mr[i]<-cs/180 #se침ales correctas
  
}





